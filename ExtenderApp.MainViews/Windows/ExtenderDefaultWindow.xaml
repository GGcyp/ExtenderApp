<v:ExtenderAppWindow x:Class="ExtenderApp.MainViews.Windows.ExtenderDefaultWindow"
                     xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                     xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                     xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                     xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                     xmlns:local="clr-namespace:ExtenderApp.MainViews.Windows"
                     xmlns:v="clr-namespace:ExtenderApp.Views;assembly=ExtenderApp.Views"
                     xmlns:vm="clr-namespace:ExtenderApp.MainViews.ViewModels"
                     mc:Ignorable="d"
                     d:DataContext="{d:DesignInstance vm:ExtenderDefaultWindowViewModel}">
    <ContentControl Content="{Binding CurrentView}"/>
</v:ExtenderAppWindow>
