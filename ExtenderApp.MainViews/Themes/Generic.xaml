<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:mvm="clr-namespace:ExtenderApp.MainViews.ViewModels"
                    xmlns:mv="clr-namespace:ExtenderApp.MainViews.Views">
    <ResourceDictionary.MergedDictionaries>
        <!--<ResourceDictionary Source="/ExtenderApp.MainViews;component/Themes/PluginTabs/PluginTab.xaml" />-->
    </ResourceDictionary.MergedDictionaries>

    <DataTemplate DataType="{x:Type mvm:MainViewModel}">
        <mv:MainView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type mvm:PluginViewModle}">
        <mv:PluginView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type mvm:MainViewRunViewModel}">
        <mv:MainViewRun />
    </DataTemplate>
</ResourceDictionary>