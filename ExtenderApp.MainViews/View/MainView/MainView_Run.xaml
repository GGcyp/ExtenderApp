<view:ExtenderAppView x:Class="ExtenderApp.MainViews.MainView_Run"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                      xmlns:local="clr-namespace:ExtenderApp.MainViews"
                      xmlns:view="clr-namespace:ExtenderApp.Views;assembly=ExtenderApp.Views"
                      mc:Ignorable="d"
                      d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid Panel.ZIndex="0">
            <ContentControl x:Name="viewContenControl"/>
        </Grid>
        <Grid x:Name="mainViewSetup" Grid.Column="0" Width="50" HorizontalAlignment="Left">
            <StackPanel VerticalAlignment="Center">
                <Button Content="返回" Click="GoHome_Click"/>
            </StackPanel>
        </Grid>
    </Grid>
</view:ExtenderAppView>
